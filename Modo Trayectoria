function varargout = Trayectorias1(varargin)
% TRAYECTORIAS1 MATLAB code for Trayectorias1.fig
%      TRAYECTORIAS1, by itself, creates a new TRAYECTORIAS1 or raises the existing
%      singleton*.
%
%      H = TRAYECTORIAS1 returns the handle to a new TRAYECTORIAS1 or the handle to
%      the existing singleton*.
%
%      TRAYECTORIAS1('CALLBACK',hObject,eventData,handles,...) calls the local
%      function named CALLBACK in TRAYECTORIAS1.M with the given input arguments.
%
%      TRAYECTORIAS1('Property','Value',...) creates a new TRAYECTORIAS1 or raises the
%      existing singleton*.  Starting from the left, property value pairs are
%      applied to the GUI before Trayectorias1_OpeningFcn gets called.  An
%      unrecognized property name or invalid value makes property application
%      stop.  All inputs are passed to Trayectorias1_OpeningFcn via varargin.
%
%      *See GUI Options on GUIDE's Tools menu.  Choose "GUI allows only one
%      instance to run (singleton)".
%
% See also: GUIDE, GUIDATA, GUIHANDLES

% Edit the above text to modify the response to help Trayectorias1

% Last Modified by GUIDE v2.5 07-Jul-2020 12:14:21

% Begin initialization code - DO NOT EDIT
gui_Singleton = 1;
gui_State = struct('gui_Name',       mfilename, ...
                   'gui_Singleton',  gui_Singleton, ...
                   'gui_OpeningFcn', @Trayectorias1_OpeningFcn, ...
                   'gui_OutputFcn',  @Trayectorias1_OutputFcn, ...
                   'gui_LayoutFcn',  [] , ...
                   'gui_Callback',   []);
if nargin && ischar(varargin{1})
    gui_State.gui_Callback = str2func(varargin{1});
end

if nargout
    [varargout{1:nargout}] = gui_mainfcn(gui_State, varargin{:});
else
    gui_mainfcn(gui_State, varargin{:});
end
% End initialization code - DO NOT EDIT


% --- Executes just before Trayectorias1 is made visible.
function Trayectorias1_OpeningFcn(hObject, eventdata, handles, varargin)
% This function has no output args, see OutputFcn.
% hObject    handle to figure
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
% varargin   command line arguments to Trayectorias1 (see VARARGIN)

imagen=imread('uno.jpg');
image(imagen,'Parent',handles.axes2);
set(handles.axes2,'XTick',[],'YTick',[]);
box on;
grid on;

[a,map]=imread('CASA3.jpg');
[r,c,d]=size(a);
x=ceil(r/140);
y=ceil(c/160);
g=a(1:x:end,1:y:end,:);
g(g==300)=5.5*300;
set(handles.pushbutton4,'CData',g);
%%%%%%%%%%%%%
global i MP
i=0;
MP=zeros(1,3);


% Choose default command line output for Trayectorias1
handles.output = hObject;

% Update handles structure
guidata(hObject, handles);

% UIWAIT makes Trayectorias1 wait for user response (see UIRESUME)
% uiwait(handles.figure1);


% --- Outputs from this function are returned to the command line.
function varargout = Trayectorias1_OutputFcn(hObject, eventdata, handles) 
% varargout  cell array for returning output args (see VARARGOUT);
% hObject    handle to figure
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)

% Get default command line output from handles structure
varargout{1} = handles.output;


% --- Executes on button press in pushbutton1.
function pushbutton1_Callback(hObject, eventdata, handles)
% hObject    handle to pushbutton1 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
global i MP
    i=i+1;
    x = str2double(get(handles.edit2, 'String'));
    y = str2double(get(handles.edit3, 'String'));
    z = str2double(get(handles.edit4, 'String'));
   
    MP(i,:)=[x,y,z]
    m=MP;
      handles.matrizalma.String = num2str(floor(m()));


% --- Executes on button press in pushbutton2.
function pushbutton2_Callback(hObject, eventdata, handles)
% hObject    handle to pushbutton2 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
cla reset
shading flat;
 rotate3d on %permite girar la sup
 xlabel('Eje X'),ylabel('Eje Y'),zlabel('Eje Z')

global i MP stop sms
iteraciones=str2double(get(handles.edit1, 'String'));
stop=true;
% while boton_stop
    while iteraciones~=0 && stop
        n=i;
        while n>0
                nf=1;
                fila=MP(nf,:);
        %         [Th1,Th2,Th3]=inversh(fila(1,1),fila(1,2),fila(1,03));
                Th1=10;Th2=10;Th3=10;
                [pasosx,pasosy,pasosz]=angles2steps(Th1,Th2,Th3);
                pack=strcat(int2str(pasosx),',',int2str(pasosy),',',int2str(pasosz),',')
        %         coordenadas=C_Directa(Th1,Th2,Th3);
                T1=Th1;T2=Th2;T3=Th3;
                prueba_directa;
                FINAL=m04(:,4);
%                 handles.edit5.String = num2str(floor(FINAL(1))); 
%                 handles.edit6.String = num2str(floor(FINAL(2)));
%                 handles.edit7.String = num2str(floor(FINAL(3))); 
                n=n-1;
                respuesta = comAM(pack)
                handles.edit5.String = num2str(floor(FINAL(1))); 
                handles.edit6.String = num2str(floor(FINAL(2)));
                handles.edit7.String = num2str(floor(FINAL(3))); 
                pause(1);
        end
        iteraciones = iteraciones-1;
%         if iteraciones==0
%             boton_stop=false;
%         else
%             boton_stop=true
%         end
    end
    if iteraciones==0
        sms='iteraciones completas'
        warndlg('Iteraciones Completadas','TRAYECTORIA')
    end
 
 
% --- Executes on button press in boton_enviar.
function boton_enviar_Callback(hObject, eventdata, handles)
% hObject    handle to boton_enviar (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)

% --- Executes on button press in pushbutton3.
function pushbutton3_Callback(hObject, eventdata, handles)
% hObject    handle to pushbutton3 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)

global stop sms
stop=false;pause(3);
sms=warndlg('Ha decidido finalizar el ciclo','STOP');




function edit1_Callback(hObject, eventdata, handles)
% hObject    handle to edit1 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)

% --- Executes during object creation, after setting all properties.
function edit1_CreateFcn(hObject, eventdata, handles)
% hObject    handle to edit1 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    empty - handles not created until after all CreateFcns called

% Hint: edit controls usually have a white background on Windows.
%       See ISPC and COMPUTER.
if ispc && isequal(get(hObject,'BackgroundColor'), get(0,'defaultUicontrolBackgroundColor'))
    set(hObject,'BackgroundColor','white');
end



